<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Version>3.0.0</Version>
    <Authors>viktor.govorov</Authors>
    <RepositoryUrl>https://github.com/govorovvs/Quarks.CQRS.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>quarks;architecture;cqrs</PackageTags>
    <PackageIcon>logo.png</PackageIcon>
    <Description>Foundations for CQRS</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\logo.png" Pack="true" PackagePath="logo.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>

</Project>
